# Tenderix Environment Variables
# Copy this to .env for local development

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
# Service role key (required for write operations)
# WARNING: In production, move write operations to server-side for security
VITE_SUPABASE_KEY=your-service-role-key-here

# n8n Webhook Base URL
VITE_WEBHOOK_BASE=https://your-n8n-instance.app.n8n.cloud/webhook

# OCR Configuration (optional)
VITE_OCR_ENABLED=false
