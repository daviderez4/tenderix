      "id": "call-claude",
      "name": "Call Claude",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [1080, 400]
    },
    {
      "parameters": {
        "jsCode": "const claude = $input.first().json;\nconst prep = $('Prepare').first().json;\n\nif (!claude.content) {\n  return [{ json: { success: false, error: 'No Claude response', data: claude }}];\n}\n\nconst text = claude.content[0]?.text || '';\nlet questions = [];\nlet parseError = null;\n\ntry {\n  const match = text.match(/\\[\\s*\\{[\\s\\S]*\\}\\s*\\]/s);\n  if (match) questions = JSON.parse(match[0]);\n  else parseError = 'No JSON array found';\n} catch (e) {\n  parseError = e.message;\n}\n\nconst safe = questions.filter(q => q.impact === 'חיובי' || q.impact === 'ניטרלי');\nconst byType = {};\nfor (const q of questions) {\n  const t = q.type || 'OTHER';\n  if (!byType[t]) byType[t] = [];\n  byType[t].push(q);\n}\n\nreturn [{\n  json: {\n    success: questions.length > 0,\n    tender_id: prep.tender_id,\n    total_questions: questions.length,\n    safe_questions: safe.length,\n    by_type: byType,\n    questions,\n    recommended: safe,\n    raw_response: text.substring(0, 400),\n    parse_error: parseError,\n    timestamp: new Date().toISOString()\n  }\n}];"
      },
